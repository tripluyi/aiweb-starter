{
  "name": "aiweb-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "./node_modules/.bin/eslint \"src/app/**/*.{js,jsx,ts,tsx}\" --ignore-pattern \"src/app/lib/\" --ignore-pattern \"src/app/shared/\"",
    "check-types": "tsc --noemit",
    "eslint:commit": "git diff --cached --name-only | grep -E 'app.*\\.[jt]sx?$' | xargs ./node_modules/.bin/eslint --quiet",
    "eslint:quiet": "./node_modules/.bin/eslint \"src/app/**/*.{js,jsx,ts,tsx}\" --ignore-pattern \"src/app/lib/\" --ignore-pattern \"src/app/shared/\" --quiet",
    "prettier:single": "./node_modules/.bin/prettier --single-quote --no-semi --trailing-comma es5 --print-width 120 --tab-width 4 --arrow-parens avoid --write",
    "prettier": "./node_modules/.bin/prettier --single-quote --no-semi --trailing-comma es5 --print-width 120 --tab-width 4 --arrow-parens avoid --write \"src/app/**/*.{js,jsx,ts,tsx}\"",
    "prettier:check": "npm run prettier -- --list-different",
    "lint-staged": "lint-staged"
  },
  "precommit": "lint-staged",
  "lint-staged": {
    "src/app/**/*.{js,jsx,ts,tsx}": [
      "npm run eslint:commit",
      "prettier --single-quote --no-semi --trailing-comma es5 --print-width 120 --tab-width 4 --arrow-parens avoid --write  \"!src/app/lib/**\" \"!src/app/shared/**\"",
      "git add"
    ]
  },
  "dependencies": {
    "@google-ai/generativelanguage": "^0.2.0",
    "@huggingface/inference": "^2.5.2",
    "@pinecone-database/pinecone": "^0.1.6",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "20.3.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@upstash/redis": "^1.21.0",
    "autoprefixer": "10.4.14",
    "body-parser": "^1.20.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "google-auth-library": "^8.8.0",
    "langchain": "^0.0.96",
    "lodash": "^4.17.21",
    "microsoft-cognitiveservices-speech-sdk": "^1.30.1",
    "next": "13.4.7",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-pdf": "^7.1.2",
    "react-redux": "^8.0.7",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.2",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.5",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/lodash": "^4.14.192",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-loader": "^9.1.2",
    "cheerio": "^1.0.0-rc.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^13.2.2",
    "pdf-loader": "^1.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.8"
  }
}
